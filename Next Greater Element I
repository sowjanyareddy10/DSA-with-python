class Solution:
    def nextGreaterElement(self, nums1: List[int], nums2: List[int]) -> List[int]:
        st=[]
        nge={}#dict
        for i in reversed(nums2):
            while st and st[-1]<=i:
                st.pop()
            if st:
                nge[i]=st[-1]
            else:
                nge[i]=-1
            st.append(i)
        ans=[]
        for i in nums1:
            ans.append(nge[i])
        return ans 
                 



        
